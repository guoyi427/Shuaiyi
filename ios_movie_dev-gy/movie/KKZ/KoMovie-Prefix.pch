// 颜色
#define RGB(r, g, b) ARGB(1.f, r, g, b)
#define ARGB(alpha, r, g, b) [UIColor colorWithRed:((r) / 255.f green:((g) / 255.f) blue:((b) / 255.f) alpha:alpha)]
#define HEX(hex) [UIColor colorWithHex:hex]
#define AHEX(alpha, hex) [UIColor colorWithHex:hex a:alpha]

